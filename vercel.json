{
  "builds": [
    {
      "src":" tic_tac_toe_ai\wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "tic_tac_toe_ai\wsgi.py" }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "CodeClauseInternship_Tic-Tac-Toe-AI.settings",
    "SECRET_KEY": "@your-secret-key",
    "DEBUG": "False"
  }
}
